#git alias
alias ga='git add'
__git_complete ga _git_add
alias gs='git status'
#alias gadd='git add'
alias glog='git log'
alias gbrn='git branch'
alias gcom='git commit -m'
alias gdf='git diff'
alias gdfn='git diff --name-only'
alias go='git checkout'
__git_complete go _git_checkout
alias gret='git reset'
alias gr='git checkout -- '
__git_complete ghead _git_checkout
alias greb='git rebase -i '
alias gpush='git push'
alias gpull='git pull''
alias gfet='git fetch --all --prune'
